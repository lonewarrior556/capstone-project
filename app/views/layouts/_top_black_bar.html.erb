<div class="just-black group">
<header class="black-bar group">


    <input id="drop-down" type="checkbox">
      <ul class="stack-exchange">
        <label for="drop-down"><img src=<%=asset_path("se-icon.png")%> alt=""/> StackExchange</label>
        <li><a href="http://stackexchange.com/"><img class="se-icon" src=<%=asset_path("se-icon.png")%> alt=""/> <i class="white-text">Stack</i><b class="blue">Exchange</b></a></li>
        <li><a href="http://stackoverflow.com/"><img class="se-icon" src=<%=asset_path("so-icon.png")%> alt=""/> Stack<b>Overflow</b></a></li>
        <li>
          <a href="http://stackexchange.com/sites"> more communities..</a></li>
      </ul>

  <ul class="header-options">
    <li>
      <% if logged_in? %>
        <a  href="<%=user_url(current_user)%>"><img class="thumb" height=30 src="<%=asset_path(current_user.avatar.url(:thumb))%>"></a>
      <% else %>
        <a  href="<%=new_user_url%>">sign up</a>
      <% end %>
    </li>
    <%= render partial: "codeBin/logout" %>

    <li class="no-login visible">
      <a href="<%=new_sessions_url%>">log in</a>
    </li>

    <li></li>

    <li>
      <ul class="help" >
        <li>help</li>
        <li>tour</li>
        <li>help center</li>
        <li>meta</li>
      </ul>
    </li>

    <li>
      <form class="search-bar" action="<%=sessions_search_url%>" method="get">
        <input  class="search-bar-box" type="text" name="search" placeholder="search">
        <button style = "display:none"></button>
      <div id="live-search-results">

      </div>
      </form>
    </li>

  </ul>


</header>
</div>
