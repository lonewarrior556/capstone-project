
<h1>login info</h1>
  <ul>
<% flash[:errors].to_a.each do |error| %>
    <li> <%=error%> </li>
<% end %>
  </ul>

<form action="<%=sessions_url%>" method="post">
  <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
  <label for="username">username
  <input type="text" name="user[username]">
  </label>
  <br>
  <label for="password">password</label>
  <input type="password" name="user[password]">
  </label>
  <br>
  <button>login</button>
</form>


<div id="goog-signin">
or  <a href="/auth/google_oauth2">sign in with <img src="http://mashable.com/wp-content/uploads/2007/06/logo-google.thumbnail.gif"/></a>
</div>

<% @projects.each do |(name, bar)| %>
<div class="style-me group">
  <li> <%= name %> </li>
  <%= bar.html_safe %>
</div>
<% end %>
