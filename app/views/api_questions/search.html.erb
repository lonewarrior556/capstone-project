<div class="static-search-results">
<h1> Users Matches for "<%= @query %>"</h1>
<ul>

<% @users.to_a.each do |user| %>

<li>
  <a href="#/users<%=user.id%>"> <%= user.username %></a>
</li>

<% end %>
</ul>



<h1> Questions Matches for "<%= @query %>"</h1>

<ul>
<% @questions.to_a.each do |question| %>


<li>
  <a href="#questions<%= question.id %>"><%= question.title %></a>
</li>


<% end %>
</ul>
<div>
